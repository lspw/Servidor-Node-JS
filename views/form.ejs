<html>
<head>
    <title>Mixer by Students CTESP</title>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <!--[if lte IE 8]><script src="assets/js/ie/html5shiv.js"></script><![endif]-->
    <link rel="stylesheet" href="css/main.css" />
</head>
<body>
    <!-- Header -->
    <header id="header">
        <h1><a href="/">Mixer</a> by Students CTESP</h1>
        <nav id="nav">
            <ul>
                <li><a href="/">Home</a></li>
                <li>
                    <a href="#" class="icon fa-angle-down">The Project</a>
                    <ul>
                        <li><a href="generic.html">What is it?</a></li>
                        <li><a href="/tech">The Technology</a></li>
                        <li><a href="contact.html">Developers</a></li>
                    </ul>
                </li>
                <li>
                    <a href="#" class="icon fa-angle-down">Hello, <%= nomeUser %></a>
					<ul>
                        <li> <a href="/logout">Logout</a> </li>
                    </ul>
                </li>
                
                <li>
                    
                </li>
            </ul>
        </nav>
    </header>
    <section id="main" class="container">

        <div class="row">
						<div class="12u">
						    <section class="box">
			                    <form action="/form" method="post">
                                    <div class="form-group">
                                        <lable>Descrição:</lable>
                                        <input type="text" class="form-control" name="descricao">
                                        <br>
                                    </div>
                                    <div class="form-group">
                                        <lable>Deposito 1: </lable>
                                        <input type="text" class="form-control" name="deposito_1">
										<br>
                                    </div>
                                    <div class="form-group">
                                        <lable>Deposito 2: </lable>
                                        <input type="text" class="form-control" name="deposito_2">
										<br>
                                    </div>
                                    <button type="submit" class="button">Insert</button>
                                </form>
						    </section>
						</div>
					</div>

        <div class="row">
        <table id="table" >  
            <thead>  
                <tr>  
                    <th>Descrição</th>
                    <th>Deposito 1 (g)</th>  
                    <th>Deposito 2 (g)</th>  
                </tr>  
            </thead>  
            <tbody>  
            <% rows.forEach(function (camp) { %>
                <tr>  
                    <td><%= camp.mix_descricao %></td> 
                    <td><%= camp.mix_dep1 %></td>  
                    <td><%= camp.mix_dep2 %></td>
                </tr>                                   
            <% }) %>
            </tbody>
        </table>
    </section>
    <footer id="footer">
					<ul class="icons">
						<li><a href="#" class="icon fa-twitter"><span class="label">Twitter</span></a></li>
						<li><a href="#" class="icon fa-facebook"><span class="label">Facebook</span></a></li>
						<li><a href="#" class="icon fa-instagram"><span class="label">Instagram</span></a></li>
						<li><a href="#" class="icon fa-github"><span class="label">Github</span></a></li>
						<li><a href="#" class="icon fa-dribbble"><span class="label">Dribbble</span></a></li>
						<li><a href="#" class="icon fa-google-plus"><span class="label">Google+</span></a></li>
					</ul>
					<ul class="copyright">
						<li>&copy; Mixer. All rights reserved.</li><li>CTESP PWDAM</li>
					</ul>
				</footer>
    </div>
    <!-- Scripts -->
			<script src="js/jquery.min.js"></script>
			<script src="js/jquery.dropotron.min.js"></script>
			<script src="js/jquery.scrollgress.min.js"></script>
